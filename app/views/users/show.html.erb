
<div class="grid_8">
	<%= render :partial => 'month_calender', :locals => { :workouts => @user.workouts } %>
</div>
<div class="grid_4">
	<div id="profile_box"><img src="<%= @user.profile_image_url %>" /><h1>@<%= @user.screen_name %></h1></div>
</div>
<div class="grid_4">
	<%= render :partial => 'user_latest_workouts', :locals => { :workouts => @user.workouts.order('created_at desc').limit(10) } %>
</div>